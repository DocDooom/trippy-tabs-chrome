{
    "name": "Trippy Tabs",
    "description": "",
    "version": "0.0.0",
    "manifest_version": 3,
    "icons": {
        "16": "./images/LSDTab_icon_16x16.png",
        "32": "./images/LSDTab_icon_32x32.png",
        "48": "./images/LSDTab_icon_48x48.png",
        "128": "./images/LSDTab_icon_128x128.png"

    },
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_popup": "./popup.html",
        "default_icons": {
            "16": "./images/LSDTab_icon_16x16.png",
            "32": "./images/LSDTab_icon_32x32.png",
            "48": "./images/LSDTab_icon_48x48.png",
            "128": "./images/LSDTab_icon_128x128.png"
        }

    },
    "options_page": "./options.html",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}